conky.config = {
   alignment = 'top_right',
   background = true,
   border_width = 10,
   cpu_avg_samples = 2,
   default_color = 'white',
   default_outline_color = 'white',
   default_shade_color = 'white',
   double_buffer = true,
   draw_borders = false,
   draw_graph_borders = true,
   draw_outline = false,
   draw_shades = false,
   use_xft = true,
   font = 'Play:size=12',
   gap_x = 10,
   gap_y = 58,
   imlib_cache_size = 10,
   minimum_height = 1380,
   minimum_width = 300,
   maximum_width = 310,
   net_avg_samples = 2,
   no_buffers = true,
   out_to_console = false,
   out_to_stderr = false,
   extra_newline = false,
   own_window = true,
   own_window_class = 'Conky',
   own_window_colour = '222222',
   own_window_type = 'desktop',
   own_window_transparent = false,
   own_window_argb_visual = true,
   own_window_hints = 'undecorated,skip_taskbar,skip_pager,below',
   stippled_borders = 0,
   text_buffer_size = 512,
   update_interval = 3.20,
