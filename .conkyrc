${voffset -10}${color bbbbbb}${font Play:size=90}${alignr}${execi 300 grep "yweather:condition" ~/.cache/weather.xml | grep -o "temp=\"[^\"]*\"" | grep -o "\"[^\"]*\"" | grep -o "[^\"]*"}${voffset -51}${font Play:bold:size=32}°C${font}${voffset -35}$color
